{
  "name": "@topplethenun/starchart",
  "packageManager": "pnpm@8.15.3",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "pnpm run build:generate && pnpm run build:icons && pnpm run build:remix",
    "build:generate": "pnpm run build:generate:env && pnpm run build:generate:star-chart-completion-schema",
    "build:generate:env": "tsx other/generate-env.ts",
    "build:generate:star-chart-completion-defaults": "tsx other/generate-star-chart-completion-defaults.ts",
    "build:generate:star-chart-completion-schema": "tsx other/generate-star-chart-completion-schema.ts",
    "build:icons": "tsx other/build-icons.ts",
    "build:remix": "remix vite:build",
    "build:server": "tsc",
    "dev": "pnpm run build:generate && pnpm run build:icons && pnpm run with-env:development node server.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "pnpm run with-env:production node server.js",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc",
    "with-env": "dotenv --",
    "with-env:development": "dotenv -v NODE_ENV=development --",
    "with-env:production": "dotenv -v NODE_ENV=production --"
  },
  "dependencies": {
